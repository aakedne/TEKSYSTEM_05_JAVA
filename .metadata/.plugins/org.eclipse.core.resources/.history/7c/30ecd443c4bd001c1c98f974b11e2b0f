package myFirstJavaProject;
 
public class welcomeToJava {

	public static void main(String[] args) {
		System.out.println("Welcome to Java!");
		
		double radius;
		double area;
		
		// assign a radius
		radius = 20;
		
		// compute area
		area = radius * radius * 3.24159;
		
		//Display results
		
		System.out.println("The result is" + " is "+ area);
		
	
		
				
		
	}

}
