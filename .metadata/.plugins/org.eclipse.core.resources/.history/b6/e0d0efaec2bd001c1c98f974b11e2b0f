package myFirstJavaProject;
 
public class welcomeToJava {

	public static void main(String[] args) {
		System.out.println("Welcome to Java!");
		
		double radius;
		double area;
		
		// assign a radius
		radius = 20;
		
		// compute area
		area = radius * radius * 3.24159;
		
		//Display results
		
		System.out.println("The results" + radius + " is "+ area);
		
		//Assignment statements
		int x = 1;
		double radius = 1.0;
		char a = 'A';
		double area = Math.PI * radius * radius;
		
				
		
	}

}
